{
  "name": "@nx-project/nest-exception",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@nx-project/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "targets": {
      "test": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "libs/ts/exception/jest.config.cts"
        }
      },
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "dist/libs/ts/exception",
          "tsConfig": "libs/ts/exception/tsconfig.lib.json",
          "packageJson": "libs/ts/exception/package.json",
          "main": "libs/ts/exception/src/index.ts",
          "assets": [
            "libs/ts/exception/*.md"
          ]
        }
      }
    }
  },
  "dependencies": {
    "tslib": "^2.3.0"
  }
}
